{
  "name": "weeworxx/shared-resources",
  "description": "Shared Laravel module with modular support for User, Post, Comments, etc.",
  "type": "library",
  "version": "dev-master",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SharedResources\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SharedResource\\SharedResourceServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1"
  },
  "repositories": [
    {
      "type": "path",
      "url": "/var/www/shared-resources",
      "options": {
        "symlink": true
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
